<?xml version="1.0"?>
<dirarchy>
    <vars>
        <var name="project_root_dir" type="gstr" regex="[a-z09_]+" />
        <var name="expr" type="gstr" />
    </vars>
    <dir path="{project_root_dir}">
        <match expr="{expr}">
            <case value="value">
                <file path="value.md" />
            </case>
            <case expr="[a-z]+">
                <file path="expr_az.md" />
            </case>
            <case expr="[0-9]+">
                <file path="expr_09.md" />
            </case>
            <case>
                <file path="default.md" />
            </case>
        </match>
    </dir>
</dirarchy>
